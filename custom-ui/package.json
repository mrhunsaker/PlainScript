    "src"
  ],
  "dependencies": {
    "@theia/core": "1.67.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "@theia/filesystem": "1.67.0",
    "@theia/navigator": "1.67.0",
    "@theia/output": "1.67.0",
    "@theia/search-in-workspace": "1.67.0",
    "@theia/terminal": "1.67.0",
    "@theia/test": "1.67.0",
    "@theia/workspace": "1.67.0",
    "rimraf": "^5.0.0",
    "typescript": "~5.4.5",
